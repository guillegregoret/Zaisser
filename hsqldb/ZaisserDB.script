CREATE MEMORY TABLE PUBLIC.RUTA(ID INTEGER PRIMARY KEY,PLANTA_ORIGEN INTEGER,PLANTA_DESTINO INTEGER,DISTANCIA_KM DOUBLE,DURACION_HS DOUBLE,PESO_MAX_KG DOUBLE,FOREIGN KEY(PLANTA_ORIGEN) REFERENCES PUBLIC.PLANTA(ID),FOREIGN KEY(PLANTA_DESTINO) REFERENCES PUBLIC.PLANTA(ID))
