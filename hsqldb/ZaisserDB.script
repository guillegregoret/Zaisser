CREATE MEMORY TABLE PUBLIC.STOCK(ID INTEGER PRIMARY KEY,INSUMO INTEGER,PLANTA INTEGER,CANTIDAD INTEGER,PUNTO_PEDIDO INTEGER,FOREIGN KEY(INSUMO) REFERENCES PUBLIC.INSUMO(ID),FOREIGN KEY(PLANTA) REFERENCES PUBLIC.PLANTA(ID))
CREATE MEMORY TABLE PUBLIC.ORDEN_PEDIDO(NRO_ORDEN INTEGER PRIMARY KEY,PLANTA_DESTINO INTEGER,FECHA_SOLICITUD DATE,FECHA_ENTREGA DATE,ESTADO VARCHAR(10),CAMION VARCHAR(7),RUTA INTEGER,COSTO_ENVIO DOUBLE,FOREIGN KEY(PLANTA_DESTINO) REFERENCES PUBLIC.PLANTA(ID),FOREIGN KEY(CAMION) REFERENCES PUBLIC.CAMION(PATENTE),FOREIGN KEY(RUTA) REFERENCES PUBLIC.RUTA(ID))
CREATE MEMORY TABLE PUBLIC.ORDEN_PEDIDO_ITEMS(NRO_ORDEN INTEGER,INSUMO INTEGER,CANTIDAD INTEGER,FOREIGN KEY(NRO_ORDEN) REFERENCES PUBLIC.ORDEN_PEDIDO(NRO_ORDEN),FOREIGN KEY(INSUMO) REFERENCES PUBLIC.INSUMO(ID))
CREATE MEMORY TABLE PUBLIC.PLANTA_CAMION(PATENTE VARCHAR(7),ID_PLANTA INTEGER,FOREIGN KEY(PATENTE) REFERENCES PUBLIC.CAMION(PATENTE),FOREIGN KEY(ID_PLANTA) REFERENCES PUBLIC.PLANTA(ID))
